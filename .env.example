# Defense Alpha Intelligence Engine - Environment Configuration
# Copy this file to .env and fill in your values

# Database (SQLite for development, PostgreSQL for production)
DATABASE_URL=sqlite:///data/defense_alpha.db
# DATABASE_URL=postgresql://user:password@localhost:5432/defense_alpha

# Application settings
DEBUG=false
LOG_LEVEL=INFO

# Data source API keys
USASPENDING_API_KEY=
CRUNCHBASE_API_KEY=
SAM_GOV_API_KEY=

# Entity resolution
FUZZY_MATCH_THRESHOLD=85
